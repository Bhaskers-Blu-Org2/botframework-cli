{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "properties": {
        "Quantity": {
            "type": "number",
            "minimum": 1,
            "maximum": 10,
            "$entities": [
                "number:Quantity",
                "number"
            ]
        },
        "Length": {
            "type": "object",
            "$entities": [
                "dimension:Length",
                "dimension"
            ],
            "required": [
                "number",
                "units"
            ],
            "properties": {
                "number": {
                    "type": "number"
                },
                "units": {
                    "type": "string"
                }
            }
        },
        "Name": {
            "type": "string",
            "$entities": [
                "personName",
                "utterance"
            ]
        },
        "Bread": {
            "type": "string",
            "enum": [
                "multiGrainWheat",
                "rye",
                "black"
            ],
            "$entities": [
                "BreadEntity"
            ]
        },
        "Hobby": {
            "type": "string",
            "$entities": [
                "utterance"
            ]
        },
        "Price": {
            "type": "object",
            "$entities": [
                "money:Price",
                "money"
            ],
            "required": [
                "number",
                "units"
            ],
            "properties": {
                "number": {
                    "type": "number"
                },
                "units": {
                    "type": "string"
                }
            }
        },
        "CancelConfirmation": {
            "type": "boolean",
            "$entities": [
                "boolean"
            ]
        },
        "ChangePropertyConfirmation": {
            "type": "boolean",
            "$entities": [
                "boolean"
            ]
        },
        "CompleteConfirmation": {
            "type": "string",
            "$entities": [
                "boolean",
                "PROPERTYName"
            ]
        },
        "PropertyToChange": {
            "type": "string",
            "enum": [
                "Quantity",
                "Length",
                "Name",
                "Bread",
                "Hobby",
                "Price"
            ],
            "$entities": [
                "PROPERTYName"
            ]
        },
        "PropertyToRemember": {
            "type": "string",
            "enum": [
                "Quantity",
                "Length",
                "Name",
                "Bread",
                "Hobby",
                "Price"
            ],
            "$entities": [
                "PROPERTYName"
            ]
        }
    },
    "required": [
        "Name",
        "Bread",
        "Hobby",
        "Price"
    ],
    "$expectedOnly": [
        "boolean",
        "PROPERTYName",
        "utterance",
        "keyPhrase",
        "personName"
    ],
    "$public": [
        "Quantity",
        "Length",
        "Name",
        "Bread",
        "Hobby",
        "Price"
    ],
    "definitions": {
        "PROPERTYName": {
            "type": "string",
            "enum": [
                "Quantity",
                "Length",
                "Name",
                "Bread",
                "Hobby",
                "Price"
            ],
            "$entities": [
                "PROPERTYName"
            ]
        }
    }
}